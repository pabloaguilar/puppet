{
	"Broadcaster": {
		"FrontendURL": "<%= @frontend_host %>"
	},
	"DB": {
		"Driver": "mysql",
		"DataSourceName": "<%= @mysql_user %>:<%= @mysql_password %>@<% if @mysql_host != "localhost" %>(<%= @mysql_host %>:3306)<% end %>/<%= @mysql_db %>"
	},
	"Metrics": {
		"Port": 6061
	},
	"Grader": {
		"BroadcasterURL": "<%= @broadcaster_host %>/broadcast/",
		"V1": {
			"Enabled": true
		}
	}
}
